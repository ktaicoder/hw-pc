  "scripts": {
    "start": "electron-forge start",
    "dev": "cross-env NODE_ENV=development ELECTRON_IS_DEV=1 NODE_OPTIONS=--max_old_space_size=32000 electron-forge start --verbose",
    "package": "electron-forge package",
    "rebuild": "electron-rebuild -f -w serialport",
    "make": "electron-forge make",
    "make_win_ia32": "electron-forge make --arch=ia32 --platform=win32",
    "make_win_x64": "electron-forge make --arch=x64 --platform=win32",
    "publish": "electron-forge publish",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --pretty --noEmit"
