{
  "name": "@ktaicoder/hw-pc",
  "productName": "AICodiny-HW",
  "version": "1.2.2",
  "description": "AI Codiny hardware electron application",
  "main": ".webpack/main",
  "scripts": {
    "postinstall": "rimraf node_modules/socket.io/client-dist",
    "cli:server": "cross-env NODE_ENV=development ts-node -r tsconfig-paths/register src/cli/websock-server",
    "dev": "cross-env NODE_ENV=development ELECTRON_IS_DEV=1 NODE_OPTIONS=--max_old_space_size=32000 electron-forge start --verbose",
    "rebuild": "electron-rebuild -f -w serialport,node-hid",
    "clean": "rimraf ./out ./settings-dev ./logs ./.webpack",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --fix \"src/**/*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"src/**/*.{ts,tsx}\"",
    "type-check": "tsc --pretty --noEmit"
  },
  "config": {
    "forge": "./forge.config.ts"
  },
